{
    "include": ["src/**/*"],
    "compilerOptions": {
      "lib": ["es2021"],
      "module": "commonjs",
      "target": "es2021",
      "strict": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "types": ["node"],
    },
  }
  